<%= form_with model: @kitten do |form| %>
    <div>
        <%= form.label :name %>
        <%= form.text_field :name %>
    </div>

    <div>
        <%= form.label :age %>
        <%= form.number_field :age %>
    </div>

    <div>
        <%= form.label :cuteness %>
        <%= form.text_field :cuteness %>
    </div>

    <div>
        <%= form.label :softness %>
        <%= form.text_field :softness %>
    </div>

    <%= form.submit %>
<% end %>